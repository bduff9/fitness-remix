{
  "name": "fitness-remix",
  "version": "0.0.1",
  "description": "Fitness tracker, written in remix.",
  "keywords": [
    "fitness",
    "health",
    "remix",
    "tailwind"
  ],
  "homepage": "https://asitewithnoname.com/",
  "license": "",
  "author": "Brian Duffey <brian.e.duffey@gmail.com>",
  "contributors": [],
  "main": "app/root.tsx",
  "repository": {
    "type": "git",
    "url": "git@github.com:bduff9/fitness-remix.git"
  },
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "codegen:db": "kysely-codegen",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "env": "sync-dotenv",
    "generate:css": "npx tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css",
    "lint:fix": "eslint . --quiet --fix",
    "lint:script:fix": "scriptlint --fix && npmPkgJsonLint .",
    "lint:style:fix": "stylelint . --fix",
    "migrate:db": "ts-node -r dotenv/config db/migration.ts",
    "prepare": "husky install",
    "setup": "typesync && npm i && npm prune",
    "start": "echo \"No start script!\"",
    "test": "echo \"No test yet!\"",
    "typecheck": "tsc",
    "validate": "run-p \"lint:**\" && tsc --noEmit && knip --no-exit-code"
  },
  "config": {},
  "dependencies": {
    "@aws-sdk/client-ses": "3.264.0",
    "@faire/mjml-react": "3.1.1",
    "@fortawesome/fontawesome-svg-core": "6.2.1",
    "@fortawesome/free-brands-svg-icons": "6.2.1",
    "@fortawesome/free-regular-svg-icons": "6.2.1",
    "@fortawesome/free-solid-svg-icons": "6.2.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@remix-run/node": "1.12.0",
    "@remix-run/react": "1.12.0",
    "@remix-run/vercel": "1.12.0",
    "@vercel/node": "2.9.0",
    "dotenv": "16.0.3",
    "kysely": "0.23.4",
    "mjml": "4.13.0",
    "nodemailer": "6.9.1",
    "pg": "8.9.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remix-auth": "3.4.0",
    "remix-auth-email-link": "1.5.2",
    "remix-auth-socials": "2.0.1",
    "remix-typedjson": "0.1.7",
    "uuid": "9.0.0",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@remix-run/dev": "1.12.0",
    "@remix-run/eslint-config": "1.12.0",
    "@remix-run/serve": "1.12.0",
    "@tailwindcss/container-queries": "0.1.0",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.9",
    "@types/eslint": "8.21.0",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/mjml": "4.7.0",
    "@types/nodemailer": "6.4.7",
    "@types/pg": "8.6.6",
    "@types/prettier": "2.7.2",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-regexp": "1.12.0",
    "husky": "8.0.3",
    "knip": "1.12.3",
    "kysely-codegen": "0.9.0",
    "lint-staged": "13.1.0",
    "npm-package-json-lint": "6.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.3",
    "scriptlint": "2.2.0",
    "stylelint": "14.16.1",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-config-tailwindcss": "0.0.7",
    "sync-dotenv": "2.7.0",
    "tailwindcss": "3.2.4",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "typesync": "0.10.0"
  },
  "engines": {
    "node": "18.x"
  },
  "private": true,
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --cache --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ],
    "package.json": [
      "scriptlint --fix",
      "npmPkgJsonLint ."
    ]
  }
}
