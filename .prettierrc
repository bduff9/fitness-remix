{
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "useTabs": false,
  "printWidth": 90,
  "arrowParens": "avoid",
  "endOfLine": "auto",
  "overrides": [
    {
      "files": [
        "**/*.ts",
        "**/*.tsx",
        "**/*.js",
        "**/*.jsx"
      ],
      "options": {
        "singleQuote": true,
        "useTabs": true
      }
    }
  ]
}
